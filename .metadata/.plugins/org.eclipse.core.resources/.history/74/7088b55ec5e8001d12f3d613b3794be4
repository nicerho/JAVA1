package sec03.exam06;

public class StringConcatExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//숫자연산
		int value = 10 + 2 + 8;
		System.out.println("value: " + value);
		//결합연산
		String str1 = 10 + 2 + "8";
		System.out.println("str1: " + str1);
		
	}

}
